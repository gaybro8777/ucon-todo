{
  "name": "ucon-todo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run fmt && npm run lint && ng build --aot",
    "build:prod": "npm run fmt && npm run lint && ng build --prod --aot --build-optimizer --progress=false",
    "test": "ng test --single-run --progress=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "fmt": "tsfmt -r",
    "swagger-ui": "cp -r node_modules/swagger-ui/dist/ ./dist/swagger-ui/ && cp ./swagger-ui-index.html ./dist/swagger-ui/index.html",
    "gen:model": "curl http://localhost:8080/api/swagger.json | sw2dts --stdin -o src/model-generated.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.3.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.1.0",
    "swagger-ui": "^3.23.11",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "^1.2.1",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "^2.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^8.0.28",
    "codelyzer": "^3.0.1",
    "jasmine-core": "^2.6.2",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.1.2",
    "sw2dts": "^2.4.0",
    "ts-node": "~3.0.4",
    "tslint": "^5.3.2",
    "typescript": "~2.3.0",
    "typescript-formatter": "^6.0.0"
  }
}
